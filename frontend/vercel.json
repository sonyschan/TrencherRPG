{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://trencherrpg-836651762884.asia-southeast1.run.app/api/:path*"
    },
    {
      "source": "/wiki",
      "destination": "https://wiki-five-lime.vercel.app/wiki/"
    },
    {
      "source": "/wiki/",
      "destination": "https://wiki-five-lime.vercel.app/wiki/"
    },
    {
      "source": "/wiki/:path*",
      "destination": "https://wiki-five-lime.vercel.app/wiki/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
