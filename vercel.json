{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://trencherrpg-836651762884.asia-southeast1.run.app/api/$1"
    },
    {
      "src": "/wiki$",
      "dest": "https://wiki-five-lime.vercel.app/"
    },
    {
      "src": "/wiki/$",
      "dest": "https://wiki-five-lime.vercel.app/"
    },
    {
      "src": "/wiki/zh-Hant/(.*)",
      "dest": "https://wiki-five-lime.vercel.app/zh-Hant/$1"
    },
    {
      "src": "/wiki/zh-Hant$",
      "dest": "https://wiki-five-lime.vercel.app/zh-Hant/"
    },
    {
      "src": "/wiki/zh-Hans/(.*)",
      "dest": "https://wiki-five-lime.vercel.app/zh-Hans/$1"
    },
    {
      "src": "/wiki/zh-Hans$",
      "dest": "https://wiki-five-lime.vercel.app/zh-Hans/"
    },
    {
      "src": "/wiki/ja/(.*)",
      "dest": "https://wiki-five-lime.vercel.app/ja/$1"
    },
    {
      "src": "/wiki/ja$",
      "dest": "https://wiki-five-lime.vercel.app/ja/"
    },
    {
      "src": "/wiki/ko/(.*)",
      "dest": "https://wiki-five-lime.vercel.app/ko/$1"
    },
    {
      "src": "/wiki/ko$",
      "dest": "https://wiki-five-lime.vercel.app/ko/"
    },
    {
      "src": "/wiki/(.*)",
      "dest": "https://wiki-five-lime.vercel.app/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ]
}
